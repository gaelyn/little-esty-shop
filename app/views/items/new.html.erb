<h2>New Item for <%= @merchant.name %></h2>
<%= form_with model: [@merchant, @item], local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :description %>
  <%= f.text_field :description %><br>

  <%= f.label :unit_price %>
  <%= f.number_field :unit_price %><br>

  <%= f.submit "Submit" %>
<% end %>
